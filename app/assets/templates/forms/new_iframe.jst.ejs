<iframe class='form-edit-box' 
  src='javascript:void(0)'
  style="  
    background-color: #e1e3dd;
    min-height: 600px;
    width: 800px;
    padding: 20px;
    margin: auto;
    display: block;">
  
  <!DOCTYPE html>
  <head>
    <style>
      .required:after {
      content: "*";
      color: red;
      }
    </style>
  </head>
  
  <body>
    <form method='post' id='form-itable'>

      <input type='hidden' name='form_id' id='form-id'>

      <div
        class="formEl"
        class="title-description"
        data-sidebar="FormEditTitle"
        style="display: block
          margin: auto;
          text-align: center">

        <h2 class="formName" 
          style="display: inline-block;
            margin: 10px auto;
            text-align: center;
            font-size: 30px;"
          >Untitled Form</h2>

        <p class="formDescription" 
          style="display: block;
            margin: 10px auto;
            text-align: center;"
          >Click here to change the form name and description</p>
      </div>
    </form>
  </body>
  
  </iframe>