<div class='message-box'>
  <h2>Results for "<%- name %>"</h2>
</div>

<div class='results-box'>
  <table class='results-table'>
    <tr class='results-header'>

      <th></th>

      <% _.each(headers, function(question){ %>
        <th><%- question %></th>
      <% }); %>
    </tr>

    <% _.each(resultsTable, function(result, index){ %>
      <tr class='results-row'>

        <td> <%= index + 1 %>
          <% if (result['new']){ %>
            *new*
          <% } %>
        </td>

        <% _.each(headers, function(question){ %>
          <td><%- (result['hash'][question] || "") %></td>
        <% }); %>

      </tr>
    <% }); %>

  </table>
</div>